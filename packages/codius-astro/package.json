{
  "name": "codius-astro",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev --port 8788",
    "start": "astro dev --port 8788",
    "build": "astro check && astro build",
    "preview": "astro build && wrangler pages dev",
    "astro": "astro",
    "deploy": "astro build && wrangler pages deploy",
    "cf-typegen": "wrangler types",
    "lint": "eslint \"./*.mjs\" \"src/**/*.{js,ts,tsx,astro}\" && prettier --check \"./*.mjs\" \"src/**/*.{js,ts,tsx,astro}\" \"migrations/*.sql\"",
    "format": "prettier --write \"./*.mjs\" \"src/**/*.{js,ts,tsx,astro}\" \"migrations/*.sql\"",
    "d1": "wrangler d1",
    "smee": "smee --path /webhooks/github/workflow-job --port 8788"
  },
  "dependencies": {
    "@astrojs/check": "^0.7.0",
    "@astrojs/cloudflare": "^11.0.1",
    "@astrojs/react": "^3.6.0",
    "@astrojs/tailwind": "^5.1.0",
    "@lucia-auth/adapter-sqlite": "^3.0.1",
    "@octokit/request-error": "^6.1.1",
    "@octokit/rest": "^21.0.0",
    "@octokit/webhooks": "^13.2.7",
    "@radix-ui/react-dropdown-menu": "^2.1.1",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-tooltip": "^1.1.2",
    "arctic": "^1.9.1",
    "astro": "^4.11.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "lucia": "^3.2.0",
    "lucide-react": "^0.399.0",
    "nanoid": "^5.0.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.2.1",
    "tailwind-merge": "^2.3.0",
    "tailwindcss": "^3.4.4",
    "tailwindcss-animate": "^1.0.7",
    "workers-qb": "^1.3.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@octokit/types": "^13.5.0",
    "@octokit/webhooks-types": "^7.5.1",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "eslint-plugin-astro": "^1.2.2",
    "prettier-plugin-astro": "^0.14.0",
    "prettier-plugin-sql-cst": "^0.11.5",
    "smee-client": "^2.0.1"
  }
}